{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.banthing.shop/api/:path*"
    },
    {
      "source": "/oauth2/:path*",
      "destination": "https://api.banthing.shop/oauth2/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}